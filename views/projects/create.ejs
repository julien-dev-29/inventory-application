        <%- include('../partials/errors.ejs') %>
            <form action="/projects/create" method="POST">
                <label for="name">Nom du projet :</label>
                <input type="text" id="name" name="name" required>
                <label for="description">Description</label>
                <textarea id="description" rows="10" name="description" required></textarea>
                <label for="github_repo">Github repo :</label>
                <input type="text" id="github_repo" name="github_repo" required></input>
                <div>
                    <button class="btn-edit" type="submit">Ajouter le projet</button>
                    <a class="btn-delete" href="/projects">Annuler</a>
                </div>
            </form>
            </body>

